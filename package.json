{
  "name": "authdeck",
  "version": "0.0.1",
  "description": "verification on chain",
  "author": "SoulNinja-dev",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "watch-tsc": "tsc -w",
    "deploy": "node dist/src/server.js",
    "watch-deploy": "nodemon dist/src/server.js",
